<div class="section">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h1 class="text-center text-primary">Perfil</h1>
        <p class="text-center">Mira tu info, edita como el mundo te ve :)</p>
      </div>
    </div>
    <div class="row text-center"></div>
    <div class="col-md-12">
      <img src="http://pingendo.github.io/pingendo-bootstrap/assets/user_placeholder.png" class="center-block img-circle img-responsive">
      <h3 class="text-center"><%=usuario.nombres%>
        <%=usuario.apellidos%></h3>
      <p class="text-center">User</p>
      <p class="text-center">
        <a href='/editarUsuario?id=<%= usuario.id %>'>
          <button type="button" class="btn btn-outline-success" style="alignment: center;">Edita tu info</button>
        </a>
      </p>

    </div>
  </div>
</div>
</div>
