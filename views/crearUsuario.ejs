<div class="section">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h1>Crear Usuario</h1>
        <form class="form-horizontal" action="/Usuario/crearUsuario" method="get">
          <div class="form-group">
            <div class="col-sm-2">
              <label for="nombres" class="col-2 col-form-label">Nombres</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="text" id="nombres" placeholder="Ingrese sus nombres" name="nombres">
            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-2">
              <label for="apellidos" class="col-2 col-form-label">Apellidos</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="text" id="apellidos" placeholder="Ingrese sus apellidos" name="apellidos">
            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-2">
              <label for="email" class="col-2 col-form-label">Email</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="email" placeholder="Ingrese su email" id="example-email-input" name="correo">
            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-2">
              <label for="password" class="col-2 col-form-label">Password</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="password" placeholder="Ingrese su contrasenia" id="example-password-input" name="password">
            </div>
          </div>

          <div class="form-group">
            <div class="col-sm-2">
              <label for="example-date-input" class="col-2 col-form-label">Date</label>
            </div>
            <div class="col-sm-10">
              <input class="form-control" type="date" value="2011-08-19" id="example-date-input" name="fechaNacimiento">
            </div>
            <div class="form-group">
              <div class="col-sm-offset-2 col-sm-10">
                <button type="reset" class="btn btn-default">Reset</button>
                <button type="submit" class="btn btn-default">Crear</button>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
